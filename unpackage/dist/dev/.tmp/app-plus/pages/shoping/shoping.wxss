
.shoping{
	width: 100%;
	height: 100%;
	background: #FFFFFF;
	position: relative;
}
.status_bar {
	height: var(--status-bar-height);
	width: 100%;
	background-image:  -webkit-gradient(linear,left top, right bottom,from(#6C37B4),to(#E4038E));
	background-image:  -o-linear-gradient(left top,#6C37B4,#E4038E);
	background-image:  linear-gradient(left top,#6C37B4,#E4038E);
	color: #FFFFFF;
}
.shoping-Scroll{
	width: 100%;
	height: calc(100% - var(--status-bar-height));
	padding-bottom: 100rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
